{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "MockServer",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": false
  },
  "exclude": ["node_modules"]
}
